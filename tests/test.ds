program myProgram:
	var a1[3][3], b:int;
  main {
    a1[0][0] = 20;
    a1[0][1] = 10;
    a1[1][0] = 30;

    b = max(a1);
    print("max: " + b);
    b = min(a1);
    print("min: " + b);
    b = mean(a1);
    print("mean: " + b);
    b = variance(a1);
    print("variance: " + b);
    b = stdev(a1);
    print("stdev: " + b);
    b = range(a1);
    print("range: " + b);
  }
  end
