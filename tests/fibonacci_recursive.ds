program myProgram:
  var n, actual: int;

  function fibonacci(cont:int, temp: int, anterior: int) :void {
    if (actual === 0) {
      actual = 1;
    }
    else {
      temp = actual;
      actual = actual + anterior;
      anterior = temp;
    }
    cont = cont + 1;
    if(cont < n){
      fibonacci(cont, temp, anterior);
    }
  }

  main {
    readline("N:", n);
    actual = 0;

    fibonacci(0, 0 , 0);
    print("Resultado:" + actual);
  }
  end